{
    "name": "duyler/scenario",
    "type": "library",
    "description": "Duyler event bus scenario handler",
    "license": "MIT",
    "keywords": [],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://duyler.org/docs/event-bus-scenario/"
    },
    "require": {
        "php": "^8.4",
        "duyler/builder": "dev-main",
        "symfony/yaml": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.77"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Duyler\\Scenario\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\Scenario\\Test\\": ["tests/"]
        }
    }
}
